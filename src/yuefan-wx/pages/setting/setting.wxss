.list {
  background: #fff;
  color: #333;
}

.item {
  display: flex;
  align-items: center;
  padding: 0 32rpx 0 40rpx;
  line-height: 107rpx;
  border-bottom: 1px solid #eee;
}

.item .item-text {
  flex: 1;
  font-size: 34rpx;
  color: #7a7a7a;
}

.item .arrow-right {
  width: 18rpx;
  height: 30rpx;
}
